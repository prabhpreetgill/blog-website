<%- include("partials/header") -%> 

<h1>Home</h1>
<p><%= startingContent %> </p>

<%posts.forEach(function(post){ %>
     <h1><%=post.title%> </h1>
     <p>
        <%=post.content + "..."%> 
        <a href= "/<%=post.title%>">Read More</a>
    </p> 
<% }) %> 

<form action="/" method="post">
    <button type="submit" class="btn btn-lg btn-primary">Add New Journal</button>
</form>

<%- include("partials/footer") -%> 
